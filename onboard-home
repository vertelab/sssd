# Onboarding home directory with autofs.

sudo apt install -y autofs sshfs

sudo mkdir -p /root/.ssh
echo "Ange lösen för lisa"
sudo scp $USER@lisa.vertel.se/home/Public/root_keys /root/.ssh

sudo wget https://raw.githubusercontent.com/vertelab/sssd/main/home.autofs /etc/auto.master.d
sudo wget https://raw.githubusercontent.com/vertelab/sssd/main/auto.home /etc/

#### DONE
echo DONE
